{
    "name": "slam/laminas-log-extensions",
    "description": "Slam extensions for laminas/laminas-log",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "laminas/laminas-log": "^2.13.0"
    },
    "require-dev": {
        "malukenho/mcbumpface": "^1.1.5",
        "phpstan/phpstan": "^0.12.64",
        "phpstan/phpstan-phpunit": "^0.12.17",
        "phpunit/phpunit": "^9.5.0",
        "slam/php-cs-fixer-extensions": "^2.1.0 || ^3.0.0",
        "slam/php-debug-r": "^1.6.1",
        "slam/phpstan-extensions": "^5.1.0",
        "thecodingmachine/phpstan-strict-rules": "^0.12.1"
    },
    "extra": {
        "mc-bumpface": {
            "stripVersionPrefixes": true
        }
    },
    "autoload": {
        "psr-4": {
            "Slam\\Laminas\\Log\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slam\\Laminas\\Log\\Tests\\": "tests/"
        }
    }
}
