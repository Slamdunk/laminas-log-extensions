{
    "name": "slam/laminas-log-extensions",
    "description": "Slam extensions for laminas/laminas-log",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "laminas/laminas-log": "^2.16.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10.33",
        "phpstan/phpstan-phpunit": "^1.3.14",
        "phpunit/phpunit": "^10.3.3",
        "slam/php-cs-fixer-extensions": "^3.8.0",
        "slam/php-debug-r": "^1.8.0",
        "slam/phpstan-extensions": "^6",
        "thecodingmachine/phpstan-strict-rules": "^1"
    },
    "autoload": {
        "psr-4": {
            "Slam\\Laminas\\Log\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slam\\Laminas\\Log\\Tests\\": "tests/"
        }
    }
}
